{
  "name": "admin-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx kill-port 5173 && vite --mode development --port 5173 --strictPort --host",
    "dev:clean": "npx kill-port 5173 && rm -rf node_modules/.vite && vite --mode development",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build": "vite build --mode production",
    "preview:dev": "vite preview --mode development",
    "preview:prod": "vite preview --mode production --port 3000",
    "preview": "vite preview --mode production",
    "serve:prod": "serve -s dist -p 3000",
    "docker:build:prod": "docker build -f Dockerfile -t admin-dashboard:prod .",
    "docker:run:prod": "docker run -d -p 3000:3000 --name admin-dashboard-prod admin-dashboard:prod",
    "docker:stop:prod": "docker stop admin-dashboard-prod && docker rm admin-dashboard-prod",
    "lint": "eslint .",
    "clean": "rm -rf dist node_modules/.vite",
    "debug:quill": "node debug-quilleditor.js",
    "test:quill-direct": "node test-quilleditor-direct.js",
    "test:quill-final": "node test-quilleditor-final.js"
  },
